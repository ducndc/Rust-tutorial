
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option _wan_defroute 'wan0'

config device 'br_lan'
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth0'
	option ipv6 '1'

config device 'eth0'
	option name 'eth0'
	option macaddr '24:0b:2a:bf:e9:80'

config interface 'lan'
	option device 'br-lan'
	option proto 'static'
	option netmask '255.255.255.0'
	option ip6assign '64'
	option ipv6 '1'
	option ipaddr '192.168.13.1'

config device 'eth1'
	option name 'eth1'
	option macaddr '24:0b:2a:bf:e9:81'

config interface 'wan0'
	option device 'eth1'
	option proto 'dhcp'
	option ipv6 '1'
	option defaultroute '1'
	option _conn_name 'DEFAULT-ROUTE'
	option _conn_type 'route'
	option _vlanid '0'

config interface 'wan0_6'
	option device 'eth1'
	option proto 'dhcpv6'
	option defaultroute '1'

config switch 'switch0'
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_port 'sw_port_0'
	option device 'switch0'
	option port '0'
	option pvid '2'

config switch_port 'sw_port_6'
	option device 'switch0'
	option port '6'
	option pvid '1'

config switch_vlan 'sw_vlan_2'
	option device 'switch0'
	option vlan '2'
	option ports '0 5'

config switch_vlan 'sw_vlan_1'
	option device 'switch0'
	option vlan '1'
	option ports '6 2 1'

config switch_port 'sw_port_1'
	option device 'switch0'
	option port '1'
	option pvid '1'

config switch_port 'sw_port_2'
	option device 'switch0'
	option port '2'
	option pvid '1'

config switch_port 'sw_port_5'
	option device 'switch0'
	option port '5'
	option pvid '2'

